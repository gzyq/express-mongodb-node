<%- include('../includes/head') %> 
<div class="main"> 
  <h1><%= title %></h1>
  <h2><a href="/">返回首页</a></h2>
  <div>用户列表</div>
   <table class="table table-hover table-bordered"> 
       <thead> 
          <tr> 
            <th>用户名</th> 
            <th>密码</th> 
            <th>权限</th> 
          </tr> 
        </thead> 
       <tbody> 
            <% users.forEach((item)=>{ %> 
               <tr > 
               <td><%= item.name %></td> 
                <td><%= item.password %></td> 
                <td><%= item.role %></td>      
               </tr> 
            <% }) %> 
        </tbody> 
    </table> 
</div> 
<%- include('../includes/foot') %>